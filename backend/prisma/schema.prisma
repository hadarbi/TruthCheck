generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AnalysisHistory {
  id        Int      @id @default(autoincrement())
  inputText      String
  sourceText     String
  analysisResult    Json
  userEmail String
  createdAt DateTime @default(now())
}
