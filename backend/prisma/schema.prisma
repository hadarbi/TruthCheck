generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AnalysisHistory {
  id        Int      @id @default(autoincrement())
  text      String
  result    String
  userEmail String   @default("guest")
  createdAt DateTime @default(now())
}
